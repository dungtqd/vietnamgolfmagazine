    "keywords": ["laravel-admin", "extension", "custom-actions-button","laravel-admin-ext"],
    "homepage": "https://github.com/nim4n136/custom-actions-button",
    "license": "MIT",
    "authors": [
        {
            "name": "NIM4N",
            "email": "nim4n136@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": ">=1.7.3"
    },
